<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qst.crop.dao.TbBankMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qst.crop.entity.TbBank">
        <id column="bank_id" property="bankId" />
        <result column="bank_name" property="bankName" />
        <result column="introduce" property="introduce" />
        <result column="bank_phone" property="bankPhone" />
        <result column="money" property="money" />
        <result column="rate" property="rate" />
        <result column="repayment" property="repayment" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        bank_id, bank_name, introduce, bank_phone, money, rate, repayment
    </sql>

</mapper>
